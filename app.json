{
  "expo": {
    "name": "GymFlow",
    "slug": "Gym-Tracking",
    "scheme": "gym-tracking",
    "version": "1.0.0",
    "icon": "./assets/icons/splash-icon-dark.png",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access to your photo library so that you can select a new profile picture"
        }
      ],
      "expo-sqlite"
    ],
    "android": {
      "package": "com.anonymous.GymTracking",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android-light.png",
        "monochromeImage": "./assets/icons/android-light.png",
        "backgroundColor": "#ffffff "
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.GymTracking",
      "icon": {
        "light": "./assets/icons/ios-light.png",
        "dark": "./assets/icons/ios-dark.png",
        "tinted": "./assets/icons/ios-tinted.png"
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "530d34db-8e74-4bb9-9a3f-894d0391f283"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/530d34db-8e74-4bb9-9a3f-894d0391f283",
      "requestHeaders": {
        "expo-channel-name": "your-channel-name"
      }
    },
    "newArchEnabled": true
  }
}
